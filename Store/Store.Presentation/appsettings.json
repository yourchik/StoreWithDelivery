{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "KafkaSettings": {
    "BootstrapServers": "your_kafka_server:port",
    "ClientId": "your_client_id",
    "GroupId": "your_group_id",
    "TopicConsume": "your_topic",
    "TopicProduce": "your_topic"
  },

  "Postgres": {
    "ConnectionString": "your_postgres_connection_string"
  }
}
